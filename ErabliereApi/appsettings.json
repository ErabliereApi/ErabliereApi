{
  "Logging": {
    "LogLevel": {
      "Default": "Warning",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Warning"
    },
    "Console": {
      "FormatterName": "json",
      "FormatterOptions": {
        "IncludeScopes": true,
        "TimestampFormat": "yyyy-MM-dd HH:mm:ss",
        "UseUtcTimestamp": false
      }
    }
  },
  "AllowedHosts": "*",
  "AzureAd": {
    "Instance": "https://login.microsoftonline.com/"
  },
  "Kestrel": {
    "AddServerHeader": false,
    "Limits": {
      "MaxRequestHeaderCount": 90
    }
  },
  "AccuWeatherBaseUrl": "http://dataservice.accuweather.com",
  "HologramBaseUrl": "https://dashboard.hologram.io",
  "IbmQuantumBaseUrl": "https://api.quantum-computing.ibm.com",
  "SWAGGER_TITLE": "ÉrablièreAPI",
  "SWAGGER_DESCRIPTION": "Un API pour assembler les informations de plusieurs appareils électronique de l'érablière",
  "SWAGGER_CONTACT_URL": "https://github.com/ErabliereApi",
  "LLMDefaultTemperature": 1
}
