{
  "$schema": "http://json.schemastore.org/launchsettings.json",
  "profiles": {
    "ErabliereApi": {
      "commandName": "Project",
      "launchBrowser": true,
      "launchUrl": "api",
      "applicationUrl": "https://0.0.0.0:5001;http://0.0.0.0:5000",
      "environmentVariables": {
        "ASPNETCORE_ENVIRONMENT": "Development",
        "USE_CORS": "True",
        "CORS_HEADERS": "*",
        "CORS_METHODS": "*",
        "CORS_ORIGINS": "*",
        "USE_SWAGGER_SERVER_SECTION": "false",
        "SWAGGER_SERVER_URL": "http://192.168.0.150:5000",
        "SWAGGER_XSRF_TOKEN_INTERCEPTOR": "false",
        "USE_SQL": "true",
        "SQL_CONNEXION_STRING": "Server=(localdb)\\mssqllocaldb;Database=ErabliereAPI;Trusted_Connection=True;",
        "SQL_USE_STARTUP_MIGRATION": "true",
        "DEBUG_HEADERS": "false",
        "LOG_SQL": "Console",
        "USE_AUTHENTICATION": "false",
        "USE_SWAGGER_AUTHORIZATIONCODE_WORKFLOW": "true",
        "USE_SWAGGER_PKCE": "true",
        "SWAGGER_AUTHORIZATION_URL": "https://security-dev.freddycoder.com/oauth2/auth",
        "SWAGGER_TOKEN_URL": "https://security-dev.freddycoder.com/oauth2/token",
        "OIDC_CLIENT_ID": "swagger-dev",
        "OAUTH2_REDIRECT_URL": "https://localhost:5001/api/oauth2-redirect.html",
        "OIDC_AUDIENCE": "http://erabliere-api",
        "OIDC_AUTHORITY": "https://security-dev.freddycoder.com/.well-known/openid-configuration"
      }
    }
  }
}
